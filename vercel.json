{
  "version": 2,
  "builds": [
    {
      "src": "Front End/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/admin.*", "dest": "Front End/dist/index.html" },
    { "src": "^/admin-dashboard.*", "dest": "Front End/dist/index.html" },
    { "src": "^/assets/(.*)", "dest": "Front End/dist/assets/$1" },
    { "src": "/(.*)\\.(.+)$", "dest": "Front End/dist/$1.$2" },
    { "src": "/(.*)", "dest": "Front End/dist/index.html" }
  ]
}
