{
  "version": 2,
  "builds": [
    {
      "src": "Back End/src/index.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "Front End/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/laporan-lingkungan/(.*)",
      "dest": "Back End/src/index.cjs"
    },
    {
      "src": "/api/artikel-lingkungan/(.*)",
      "dest": "Back End/src/index.cjs"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "Back End/src/index.cjs"
    },
    {
      "src": "/api/visitor/(.*)",
      "dest": "Back End/src/index.cjs"
    },
    {
      "src": "/admin-dashboard",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/artikel-table",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/laporan-table",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/tambah-artikel",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/update-artikel/.*",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/login",
      "dest": "Front End/dist/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "Front End/dist/assets/$1"
    },
    {
      "src": "/[^.]+",
      "dest": "Front End/dist/index.html",
      "status": 200
    }
  ]
}
